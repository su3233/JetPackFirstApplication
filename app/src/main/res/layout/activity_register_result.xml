<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".registeractivity.RegisterResultActivity">

    <Button
        android:id="@+id/bt_register_for_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="启动activity并返回结果" />

    <TextView
        android:id="@+id/tv_register_for_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="显示跳转后结果" />

    <FrameLayout
        android:id="@+id/content_fragment_result"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1" />
</androidx.appcompat.widget.LinearLayoutCompat>